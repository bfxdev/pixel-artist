{
  "name": "pixel-artist",
  "version": "1.0.0",
  "description": "Create pixel-art from input pictures",
  "main": "pixel-artist.js",
  "scripts": {
    "test": "npm run build:lib && mocha -r ts-node/register src/test.ts",
    "start": "npm run clean && npm run build",
    "clean": "npm run clean:lib && npm run clean:bundle && npm run clean:test-results",
    "clean:lib":  "cd lib  && del *.ts && del *.js && del *.map && cd .. ",
    "clean:bundle": "cd bundle && del *.ts && del *.js && del *.map && cd .. ",
    "clean:test-results": "cd test-results && del *.png && del *.jpg && cd ..",
    "build": "npm run build:lib && npm run build:bundle && npm run build:webpack && npm run build:doc",
    "build:lib": "tsc -p .",
    "build:bundle": "tsc -m es6 --outDir bundle -p .",
    "build:doc": "typedoc",
    "build:webpack": "webpack --config webpack.config.min.js && webpack --config webpack.config.bundle.js",
    "build:with-pre-clean": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bfxdev/pixel-artist.git"
  },
  "keywords": [
    "pixel art",
    "jimp",
    "image processing",
    "outline",
    "dithering"
  ],
  "author": "bfxdev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bfxdev/pixel-artist/issues"
  },
  "homepage": "https://github.com/bfxdev/pixel-artist#readme",
  "dependencies": {
    "jimp": "^0.6.4",
    "ts-node": "^8.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/jimp": "^0.2.28",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.7",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "typedoc": "^0.15.0",
    "typescript": "^3.5.1",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8"
  }
}
